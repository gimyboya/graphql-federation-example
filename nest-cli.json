{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/graphql-gateway/src",
  "monorepo": true,
  "root": "apps/graphql-gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/graphql-gateway/tsconfig.app.json"
  },
  "projects": {
    "graphql-gateway": {
      "type": "application",
      "root": "apps/graphql-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/graphql-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphql-gateway/tsconfig.app.json"
      }
    },
    "pandas-api": {
      "type": "application",
      "root": "apps/pandas-api",
      "entryFile": "main",
      "sourceRoot": "apps/pandas-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pandas-api/tsconfig.app.json"
      }
    },
    "users-api": {
      "type": "application",
      "root": "apps/users-api",
      "entryFile": "main",
      "sourceRoot": "apps/users-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-api/tsconfig.app.json"
      }
    },
    "posts-api": {
      "type": "application",
      "root": "apps/posts-api",
      "entryFile": "main",
      "sourceRoot": "apps/posts-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/posts-api/tsconfig.app.json"
      }
    }
  }
}